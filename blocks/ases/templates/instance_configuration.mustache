{{#data}}
<div class="container">
    <div class="row">
        <h2>GESTIÓN DE ADMNISTRADORES DEL SISTEMA</h2>
        <hr/>
        <div class="panel-group">
            <div class="panel panel-default">
                <div class="panel-heading">
                  <h4 class="panel-title">
                    <a data-toggle="collapse" href="#collapse1">Asignación Adminstracion del Sistema</a>
                  </h4>
                </div>{{!--end panel-heading--}}
                <div id="collapse1" class="panel-collapse collapse">
                  <div class="panel-body">
                    
                        <form class="form-pilos" id="role_man_form">
                            <div >
                                <ul>
                                    <li>
                                        <label for="username_input">Nombre de usuario:</label> 
                                        <input type="text" name="username_input" id="username_input"/>
                                        <button class="submit" type="button" id="search_button" >Buscar</button> 
                                    </li>
                                    <li class="assignment_li"> 
                                        <label for="name_lastname">Nombre completo: </label>
                                        <input type="text" id="name_lastname" name="name_lastname" disabled />
                                        <input type="text" class="hide" id="user_id" name="user_id"/>
                                    </li> 
                                    <li class="assignment_li">
                                        <label for="lista_programas">Programa</label>
                                        <select name="lista_programas" id="lista_programas" disabled>
                                            {{{programs}}}
                                        </select>
                                    </li>
                                    {{!--<li class="assignment_li" style="display:none;" id="boss_li" >--}} 
                                    {{!--    <label for="boss_select">Jefe</label>--}}
                                    {{!--    <select name="boss_select" id="boss_select">--}}
                                          
                                    {{!--    </select>--}}
                                    {{!--</li>--}}
                                </ul>
                                
                            </div>{{!--end col-md-6--}}
                        </form>
                            <div id="segumientos_div" class="assignment_li">
                                <h4>Seguimientos:</h4>
                                    <div class="row col-md-offset-1">
                                        <label class="col-md-2">Academico:</label>
                                        <label class="radio-inline" id="1">
                                          <input type="radio" name="segAca"  value="1">Si
                                        </label>
                                        <label class="radio-inline" id="1">
                                          <input type="radio" name="segAca"  value="0">No
                                        </label>
                                    </div>
                                    <div class="row col-md-offset-1">
                                        <label class="col-md-2" >Asistencias:</label>
                                        <label class="radio-inline" id="1">
                                          <input type="radio" name="segAsis"  value="1">Si
                                        </label>
                                        <label class="radio-inline" id="1">
                                          <input type="radio" name="segAsis"  value="0">No
                                        </label>
                                    </div>
                                    <div class="row col-md-offset-1">
                                        <label class="col-md-2">Socioeducativo:</label>
                                        <label class="radio-inline" id="1">
                                          <input type="radio" name="segSoc"  value="1">Si
                                        </label>
                                        <label class="radio-inline" id="1">
                                          <input type="radio" name="segSoc"  value="0">No
                                        </label>
                                    </div>
                            </div>
                            <div class="col-md-3 col-md-offset-5 assignment_li">
                                <button class="submit btn-form" type="button" id="ok_button">Aceptar</button>
                                <button class="submit btn-form" type="button" id="cancel_button">Cancelar</button>
                            </div>{{!--end div buttons --}}
                            <div class="col-md-4 col-md-offset-8" id="next_div" style="display:none;">
                                <button class="submit btn-form" type="button" id="next">Cargar Informacion Básica</button>
                            </div>
                    {{!--end panel-body--}}
                        </form>
                    </div>
            </div>{{!--end panel-collapse1--}}
        <div class="panel panel-default">
            <div class="panel panel-default">
                <div class="panel-heading">
                  <h4 class="panel-title" id="list-users-panel">
                    <a data-toggle="collapse" id="listadministradores" href="#collapse2">Lista de Administradores</a>
                  </h4>
                </div>{{!--end panel-heading--}}
                <div id="collapse2" class="panel-collapse collapse">
                    <div class="panel-body">
                        <h2>Lista de Administradores</h2>
                    </div>
                <div class="col-md-8 col-md-offset-2" id="div_users">
                    
                </div>
                </div>
            </div>
        </div>{{!--end panel-default--}}
        
    </div>{{!--end panel-group--}}
</div>

<script type="text/javascript" src="../amd/require-setup.js"></script>
<script data-main="../amd/src/instance_configuration/amd_instance_configuration" src="../amd/scripts/require.js"></script>
{{/data}}